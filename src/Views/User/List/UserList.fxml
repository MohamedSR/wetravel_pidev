<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Views.User.List.UserListController">
    <stylesheets>
        <URL value="@userlist.css" />
    </stylesheets>
   <children>
      <TableView fx:id="UserList" layoutY="37.0" prefHeight="314.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Nom" />
          <TableColumn fx:id="roleCol" prefWidth="75.0" text="role" />
          <TableColumn fx:id="emailCol" prefWidth="75.0" text="email" />
          <TableColumn fx:id="phoneCol" prefWidth="75.0" text="phone" />
          <TableColumn prefWidth="75.0" text="Actions" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="8.0" layoutY="3.0" mnemonicParsing="false" onAction="#goToAddUser" prefHeight="32.0" prefWidth="143.0" text="Ajouter un utilisateur" />
      <Button layoutX="14.0" layoutY="354.0" mnemonicParsing="false" onAction="#backToHome" prefHeight="32.0" prefWidth="143.0" text="Annuler" />
   </children>
</AnchorPane>
