    <stylesheets>
        <URL value="@userlist.css" />
    </stylesheets>
   <children>
      <TableView fx:id="UserList" layoutY="37.0" prefHeight="363.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Nom" />
          <TableColumn fx:id="roleCol" prefWidth="75.0" text="role" />
          <TableColumn fx:id="emailCol" prefWidth="75.0" text="email" />
          <TableColumn fx:id="phoneCol" prefWidth="75.0" text="phone" />
          <TableColumn prefWidth="75.0" text="Actions" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="8.0" layoutY="3.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="143.0" text="Ajouter un utilisateur" />
   </children>
</AnchorPane>
